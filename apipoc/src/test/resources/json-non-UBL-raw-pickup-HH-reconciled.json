{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "_id": {
      "type": "object",
      "properties": {
        "$oid": {
          "type": "string"
        }
      },
      "required": [
        "$oid"
      ]
    },
    "shipments": {
      "type": "array",
      "additionalProperties": false,
      "items": [
        {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "Shipment": {
              "type": "object",
              "additionalProperties": false,
              "properties": {
                "Consignment": {
                  "type": "object",
                  "additionalProperties": false,
                  "properties": {
                    "CarrierAssignedID": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "CarrierAssignedID"
                  ]
                },
                "ReconciledShipmentID": {
                  "type": "string"
                },
                "ReconciledShipmentOperation": {
                  "type": "string"
                }
              },
              "required": [
                "Consignment",
                "ReconciledShipmentID",
                "ReconciledShipmentOperation"
              ]
            }
          },
          "required": [
            "Shipment"
          ]
        }
      ]
    },
    "datetime": {
      "type": "string"
    },
    "feed-source": {
      "type": "string"
    }
  },
  "required": [
    "_id",
    "shipments",
    "datetime",
    "feed-source"
  ]
}